if(Meteor.isServer){
		Meteor.methods({
			'insertSalespromotion': function(salespromotionDoc){
				//var currentUserId = Meteor.userId();
				Coupons.insert(salespromotionDoc);
			},
			
			'updateSalespromotion':function(id,salespromotionDoc){
				Coupons.update(id, {$set:salespromotionDoc});
			}
		});
	};